    {
      "id": 15,
      "title": "CWV Pass / Fail",
      "description": "Sites passing ALL 3 core web vitals (LCP ≤ 2500ms, INP ≤ 200ms, CLS ≤ 0.10) vs sites failing any",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 6 },
      "datasource": { "type": "grafana-bigquery-datasource", "uid": "dealeron-bigquery" },
      "targets": [
        {
          "refId": "A",
          "datasource": { "type": "grafana-bigquery-datasource", "uid": "dealeron-bigquery" },
          "rawSql": "SELECT\n  COUNTIF(p75_lcp <= 2500 AND p75_inp <= 200 AND p75_cls <= 0.10) as Pass,\n  COUNTIF(NOT (p75_lcp <= 2500 AND p75_inp <= 200 AND p75_cls <= 0.10)) as Fail\nFROM (\n  SELECT origin,\n    AVG(p75_lcp) as p75_lcp,\n    AVG(p75_inp) as p75_inp,\n    AVG(p75_cls) as p75_cls\n  FROM `dealeron-crux.dealeron_crux.cwv_monthly`\n  WHERE yyyymm = (SELECT MAX(yyyymm) FROM `dealeron-crux.dealeron_crux.cwv_monthly`)\n  AND device IN (${device:sqlstring})\n  AND p75_lcp IS NOT NULL AND p75_inp IS NOT NULL AND p75_cls IS NOT NULL\n  GROUP BY origin\n)",
          "rawQuery": true,
          "editorMode": "code",
          "format": 1
        }
      ],
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "Pass" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }
            ]
          },
          {
            "matcher": { "id": "byName", "options": "Fail" },
            "properties": [
              { "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }
            ]
          }
        ]
      },
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": true },
        "pieType": "donut",
        "tooltip": { "mode": "multi", "sort": "none" },
        "legend": { "displayMode": "list", "placement": "right", "values": ["value", "percent"] }
      }
    },
    {
